{
  "name": "vscan-security-scanner",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "./dist-electron/main/index.js",
  "productName": "vScan Security Scanner",
  "description": "Security Scanner for Backup",
  "author": {
    "name": "Marco Escobar",
    "email": "marco.escobar@gmail.com"
  },
  "scripts": {
    "dev": "vite",
    "copy-manifest": "copy resources\\app.manifest dist-electron\\",
    "dev:backend": "cd server && npm run dev",
    "compile:scripts": "tsc -p scripts/tsconfig.json",
    "dev:frontend": "vite",
    "build": "tsc --noEmit && vite build",
    "build:preload": "tsx scripts/build-preload.ts",
    "build:electron": "tsc -p electron/tsconfig.json && npm run build:preload",
    "build:electron:script": "tsx scripts/build-electron.ts",
    "electron:start": "cross-env NODE_ENV=development electron .",
    "electron:dev": "concurrently -k -n=backend,frontend,electron \"npm run dev:backend\" \"vite\" \"wait-on tcp:5173 tcp:3001 && npm run build:electron && npm run electron:start\"",
    "build:win": "npm run build && npm run build:electron && electron-builder --win --x64 --config electron-builder.json",
    "build:prepare": "node scripts/build-electron.ts && node scripts/build-preload.ts",
    "build:backend": "cd server && npm run build",
    "pack:backend": "tsx scripts/pack-backend.ts",
    "build:all": "rimraf dist dist-electron resources/server && npm run build && npm run build:electron && npm run build:backend && npm run pack:backend",
    "clean": "rimraf dist dist-electron resources/server",
    "make:portable": "cross-env NODE_ENV=production npm run build:all && npm run copy-manifest && electron-builder --win --x64 --config electron-builder.json -c.target=portable",
    "build:portable": "npm run build:all && electron-builder --win --x64 --config electron-builder.json --dir",
    "make:installer": "cross-env NODE_ENV=production npm run build:all && electron-builder --win --x64 --config electron-builder.json -c.target=nsis"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@remix-run/router": "^1.15.1",
    "@tanstack/react-query": "^5.28.4",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "electron-log": "^5.2.4",
    "lucide-react": "^0.358.0",
    "node-powershell": "^5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.0.2",
    "recharts": "^2.13.3",
    "ssh2": "^1.16.0",
    "tailwind-merge": "^2.2.1",
    "uuid": "^11.0.5",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@babel/types": "^7.26.5",
    "@types/electron": "^1.6.12",
    "@types/fs-extra": "^11.0.4",
    "@types/history": "^4.7.11",
    "@types/node": "^20.17.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/sqlite3": "^3.1.11",
    "@types/ssh2": "^1.15.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "8.20.0",
    "@typescript-eslint/parser": "8.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^34.0.1",
    "electron-builder": "^25.1.8",
    "esbuild": "^0.21.5",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "fs-extra": "^11.2.0",
    "postcss": "^8.4.49",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
